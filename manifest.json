{
  "manifest_version": 3,
  "name": "Kibana JSON Message Formatter",
  "version": "1.1",
  "description": "Parses JSON in the 'message' field of Kibana logs and displays it formatted.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Configure JSON Fields"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
